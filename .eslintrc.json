{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "amd": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-cond-assign": [
      2,
      "always"
    ],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-console": 2,
    "no-constant-condition": 2,
    "padded-blocks": 0,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "default-case": 2,
    "no-undef": "warn",
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "no-fallthrough": 2,
    "no-octal": 2,
    "no-redeclare": [
      2,
      {
        "builtinGlobals": true
      }
    ],
    "no-delete-var": 2,
    "no-unused-vars": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": [
      2,
      true
    ],
    "no-process-exit": 2,
    "no-sync": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-continue": 2,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "no-unneeded-ternary": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi-spacing": [
      2,
      {
        "after": false
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "sort-vars": [
      2,
      {
        "ignoreCase": false
      }
    ],
    "space-in-parens": [
      2,
      "never",
      {}
    ],
    "space-infix-ops": 2,
    "spaced-comment": [
      2,
      "always",
      {}
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "no-const-assign": 2,
    "no-var": 2,
    "prefer-const": 2,
    "prefer-spread": 2
  }
}